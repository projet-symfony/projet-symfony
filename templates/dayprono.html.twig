{%  extends "base.html.twig" %}

{% block title  %}
    Nextipster - Pronostic du jour
{% endblock %}


{%  block stylesheets %}
    <link rel="stylesheet" type="text/css" href="{{ asset('CSS/dayprono.css') }}">
{% endblock %}

{%  block body %}

    <h2> Pronostic du jour </h2>

    <hr class="separator">
    <ul>
    {% for v in vote %}

                        <li>
                            <p class="detailMatch"> m.datetime</p>
                            <p class="teamMatch"> m.club1 - m.club2 </p>
                            <div class="result">
                                <button class="activeButton"> 1 </button>
                                <button class="otherButton" disabled> N </button>
                                <button class="otherButton" disabled> 2 </button>
                            </div>

                            <div class="cote">

                                              <span class="activeCote">{{   v.Vote1 }} </span>


                                                 <span class="otherCote">V.VoteN</span>


                                                    <span class="otherCote">V.vote2</span> </div>




                        </li>
            {% endfor %}


    </ul>

{% endblock %}